{
  "name": "membersonly-odin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon app.js",
    "start": "NODE_ENV=production node app.js",
    "db:which": "dotenv -e .env -- bash -lc 'psql \"$DATABASE_URL\" -c \"\\\\conninfo\"'",
    "db:tables": "dotenv -e .env -- bash -lc 'psql \"$DATABASE_URL\" -c \"\\\\dt *.*\"'",
    "db:init":   "dotenv -e .env -- bash -lc 'psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 -f db/schema.sql'",
    "db:reset": "dotenv -e .env -- psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 -f db/reset.sql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albertcastillom/membersonly-odin.git"
  },
